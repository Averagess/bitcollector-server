{
  "name": "bitcollector-server",
  "packageManager": "yarn@3.3.1",
  "version": "1.1.0",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "build": "tsc",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --ignore-watch node_modules ./src/server.ts",
    "test": "jest src/",
    "test:watch": "jest --watch",
    "populateDB": "cross-env NODE_ENV=development ts-node ./src/scripts/populateDevData.ts",
    "lint": "eslint --ext .ts src/"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongoose": "^6.8.3",
    "mongoose-long": "^0.6.0",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.2",
    "pg": "^8.8.0",
    "rotating-file-stream": "^3.0.4",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.18",
    "@types/node-cron": "^3.0.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^8.33.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.2",
    "superagent": "^8.0.6",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  }
}
