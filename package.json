    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "mongoose": "^6.10.0",
    "mongoose-long": "^0.7.0",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.2",
    "pg": "^8.11.0",
    "redis": "^4.6.7",
    "rotating-file-stream": "^3.1.0",
    "winston": "^3.9.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.2.5",
