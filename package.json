{
  "name": "bitcollector-server",
  "packageManager": "yarn@3.3.1",
  "version": "1.2.1",
  "scripts": {
    "start": "node ./build/server.js",
    "build": "tsc",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --ignore-watch node_modules ./src/server.ts",
