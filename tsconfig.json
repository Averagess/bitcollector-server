{
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "moduleResolution": "node",
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "outDir": "build",
    "baseUrl": "./src",
  },
  "exclude": [
    "node_modules",
    "build"
  ],
  "include": [
    "src/*",
  ],
  "files": [
    "src/server.ts"
  ]
}